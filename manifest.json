{
  "name": "Kudoser",
  "description": "automates kudosing on a popular fitness social media platform",
  "version": "1.2.1",
  "icons": 
  {
	  "16":"/icons/16.png",
	  "32":"/icons/32.png",
	  "64":"/icons/64.png",
	  "128":"/icons/128.png"
  },
  "manifest_version": 3,
  "background": {"service_worker": "background.js"},
  "permissions": ["storage", "activeTab", "scripting"],
  "action": 
  {
	  "default_popup": "popup.html",
	  "default_icon": 
	  {
		  "16":"/icons/16.png",
		  "32":"/icons/32.png",
		  "64":"/icons/64.png",
		  "128":"/icons/128.png"
	  }
  }
}